{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug FastAPI (Poetry)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "focus_track_api.app:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--port",
        "8000"
      ],
      "jinja": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
